<?xml version="1.0" encoding="UTF-8"?>
<groups xmlns="http://www.isdc.ro/wro"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.isdc.ro/wro wro.xsd">
    <!--Following xml contains one group called main.
    The group contains all css, less and JavaScript
    files from /resources/ directory and its sub-directories:-->
    <group name="main">
        <css>/static/resources/**.css</css>
        <css>/static/resources/**.less</css>
        <js>/static/resources/**.js</js>
    </group>

</groups>